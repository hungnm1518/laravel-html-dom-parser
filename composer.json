{
    "name": "hungnm1518/laravel-html-dom-parser",
    "description": "Laravel wrapper for the PHP Simple HTML DOM Parser package",
    "keywords": [
        "dom parser",
        "html dom parser",
        "laravel"
    ],
    "require": {
    	"php": "^8.0",
        "illuminate/support": "^8.0|^9.0|^10.0|^11.0",
        "kub-at/php-simple-html-dom-parser": "^1.8"
    },
    "homepage": "https://github.com/hungnm1518/laravel-html-dom-parser",
    "license": "MIT",
    "authors": [
		{
		  "name": "Hung Nguyen",
		  "homepage": "http://fb.com/hungnm1518/",
		  "email": "hungnm144@gmail.com",
		  "role": "Developer"
		}
	],
	"homepage": "http://fb.com/hungnm1518/",
	"support": {
		"email": "hungnm144@gmail.com",
		"issues":"https://github.com/hungnm1518/laravel-html-dom-parserissues",
		"wiki":"https://github.com/hungnm1518/laravel-html-dom-parser",
		"source":"https://github.com/hungnm1518/laravel-html-dom-parser",
		"docs": "https://github.com/hungnm1518/laravel-html-dom-parser"
	},
    "autoload": {
        "psr-4": { "HungNM\\LaravelHtmlDomParser\\": "" }
    },
    "extra": {
        "laravel": {
            "providers": [
                "HungNM\\LaravelHtmlDomParser\\ServiceProvider"
            ],
            "aliases": {
                "HTMLDomParser": "HungNM\\LaravelHtmlDomParser\\Facade"
            }
        }
    }
}
