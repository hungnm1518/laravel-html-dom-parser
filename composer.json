{
    "name": "hungnm144/laravel-html-dom-parser",
    "description": "Laravel wrapper for the PHP Simple HTML DOM Parser package",
    "keywords": [
        "dom parser",
        "html dom parser",
        "laravel"
    ],
    "require": {
    	"php": ">=7.4",
        "illuminate/support": "^8.0",
        "kub-at/php-simple-html-dom-parser": "^1.8"
    },
    "homepage": "https://github.com/hungnm1518/html-dom-parser",
    "license": "MIT",
    "authors": [
        {
            "name": "Hung Nguyen",
            "email": "hungnm144@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": { "HungNM\\LaravelHtmlDomParser\\": "" }
    },
    "extra": {
        "laravel": {
            "providers": [
                "HungNM\\LaravelHtmlDomParser\\ServiceProvider"
            ],
            "aliases": {
                "HTMLDomParser": "HungNM\\LaravelHtmlDomParser\\Facade"
            }
        }
    }
}
